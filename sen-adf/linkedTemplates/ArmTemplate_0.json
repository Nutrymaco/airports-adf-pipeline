					"fileName": {
						"type": "string",
						"defaultValue": "fileName"
					}
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureDataLakeStoreLocation",
						"fileName": {
							"value": "@concat(dataset().fileName, '.csv')",
							"type": "Expression"
						},
						"folderPath": {
							"value": "@concat(dataset().pipeiineName, '/', dataset().stage, '/')",
