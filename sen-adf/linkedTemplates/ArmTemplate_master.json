{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sen-adf"
		},
		"AzureDataLakeAnalytics1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataLakeAnalytics1'"
		},
		"AzurePostgreSql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzurePostgreSql1'"
		},
		"GoshaBD_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'GoshaBD'"
		},
		"GOSH_PSQL_CONN_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'GOSH_PSQL_CONN'"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "sendla"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "15f33613-db8a-4a49-819e-a352da33bbc5"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "8957652c-8b32-4246-9daa-700cb6f31fc6"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "a66ba509-6b33-4f11-b3f8-8547f92c3805"
		},
		"AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "sen-mirea-rg"
		},
		"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://sendls.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "8957652c-8b32-4246-9daa-700cb6f31fc6"
		},
		"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "a66ba509-6b33-4f11-b3f8-8547f92c3805"
		},
		"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "sen-mirea-rg"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeAnalytics1_servicePrincipalKey": {
						"value": "[parameters('AzureDataLakeAnalytics1_servicePrincipalKey')]"
					},
					"AzurePostgreSql1_connectionString": {
						"value": "[parameters('AzurePostgreSql1_connectionString')]"
					},
					"GoshaBD_connectionString": {
						"value": "[parameters('GoshaBD_connectionString')]"
					},
					"GOSH_PSQL_CONN_connectionString": {
						"value": "[parameters('GOSH_PSQL_CONN_connectionString')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_accountName')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeAnalytics1_properties_typeProperties_resourceGroupName')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}